<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset={charset}">
{themeCSS}
<script type="text/javascript" src="{site_url}/js/swfobject.js"></script>
<script type="text/javascript">
		// <![CDATA[
		function divResize(id, nw, nh) {
			var obj = document.getElementById(id);
			obj.style.width = nw + "px";
			obj.style.height = nh + "px";
		}
		function divColor(id,color) {
		    var obj = document.getElementById(id);
		    obj.style.backgroundColor = color;
	    }
		// ]]>
</script>
<title>{title}</title>
</head>
<body>
<script type="text/javascript">
	var flashvars = {};
    var params = {
    wmode: "transparent"
    };
	var attributes = {};
	flashvars.mp3Url="{movie}";
	flashvars.title="{mp3_title}";
	flashvars.allowDl="{allow_download}";
	flashvars.thumbUrl="{u_tn}";
	flashvars.langArtist="{lang_artist}";
	flashvars.langAlbum="{lang_album}";
	flashvars.langSong="{lang_song}";
	flashvars.langTrack="{lang_track}";
	flashvars.langGenre="{lang_genre}";
	flashvars.langYear="{lang_year}";
	flashvars.langDownload="{lang_download}";
	flashvars.langInfo="{lang_info}";
	flashvars.autoplay="{autostart}";
    swfobject.embedSWF("{site_url}/players/MGmp3.swf", "mp3audio2", "525", "292", "9.0.0", "expressInstall.swf", flashvars, params, attributes);
</script>
<div id="mp3audio2">
	<a href="http://www.adobe.com/go/getflashplayer">
		<img src="http://www.adobe.com/images/shared/download_buttons/get_flash_player.gif" alt="Get Adobe Flash player" />
	</a>
</div>
</body>
</html>
{# end {templatelocation} #}
